{
  "entries" : [
  {% for piece in pieces %}
    {
      "slug"       : "{{ piece.slug }}",
      "image" : "{% if IS_DEV %}http://127.0.0.1:8000/site_media{% else %}http://sbm.art72.org{% endif %}/media/{{ piece.default_image }}",
      "title"       : "{{ piece.title }}",
      "date"        : "{{ piece.date }}",
      "time"        : "{{ piece.time }}",
      "theme"       : "{{ piece.theme }}",
      "type"        : "{{ piece.type }}",
      "location"    : "{{ piece.location }}",
      "description" : "{{ piece.description }}"
    {% if forloop.last %}
      }
    {% else %}
      },
    {% endif %}
  {% endfor %}
]
}
