{
  "entries" : [
  {% for piece in pieces %}
    {
      "slug"       : "{{ piece.slug }}",
      "image" : "{% if IS_DEV %}http://127.0.0.1:8000{% else %}http://sbm.art72.org{% endif %}/media/{{ piece.default_image }}",
      "title"       : "{{ piece.title }}",
      "name"        : "{{ piece.name }}",
      "year"        : "{{ piece.year }}",
      "location"    : "{{ piece.location }}",
      "description" : "{{ piece.description }}"
    {% if forloop.last %}
      }
    {% else %}
      },
    {% endif %}
  {% endfor %}
]
}
